# Backpack交易所API配置 - 使用ED25519密钥对
# 注意: 这些必须是Base64编码的ED25519密钥
# 请将此文件复制为 config.env 并填入您的真实API密钥

BACKPACK_API_KEY=YOUR_BACKPACK_API_KEY_HERE
BACKPACK_PRIVATE_KEY=YOUR_BACKPACK_PRIVATE_KEY_HERE

# 交易配置
MAX_POSITION_SIZE=0.1
STOP_LOSS_PCT=0.02
TAKE_PROFIT_PCT=0.04
MIN_ORDER_SIZE=10.0

# 交易对配置 (用逗号分隔)
TRADING_PAIRS=SOL_USDC,BTC_USDC,ETH_USDC

# 日志级别
LOG_LEVEL=INFO
